unit uITypeButton;

interface

uses Vcl.Graphics, Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.ExtCtrls;

type
  tTypeButton = (tyPrimary, tySecondary, tySucces, tyDanger, tyWarning, tyDark);

  ITypeButton = interface

    procedure Enter(sender: TObject);
    procedure Leave(sender: TObject);
    function New : ITypeButton;
  end;


implementation

end.
